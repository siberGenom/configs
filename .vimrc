set nocompatible              " be iMproved, required
filetype on                 " required
filetype plugin indent on    " required
syntax on 
colorscheme zenburn
set encoding=utf-8
set number
set tabstop=4
set shiftwidth=4
set expandtab
set visualbell

" l - undo
" y - insert
noremap d g
noremap e k
noremap f e
noremap g t
noremap j y
noremap k n
noremap l u
noremap p r
noremap r s
noremap s d
noremap t f
noremap u i
noremap y o
noremap D G
noremap E K
noremap F E
noremap G T
noremap I L
noremap J Y
noremap K N
noremap N J
noremap O P
noremap P R
noremap R S
noremap S D
noremap T F
noremap U I
noremap Y O

noremap n h
noremap e j
noremap i k
noremap o l

noremap dd gg
noremap ; p
